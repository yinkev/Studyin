{
  "name": "oms1-upper-limb-arcade",
  "version": "0.1.0",
  "description": "Evidence-first, deterministic study arcade scaffold for OMS-1 upper limb",
  "type": "module",
  "scripts": {
    "validate:items": "node scripts/validate-items.mjs",
    "analyze": "node scripts/analyze.mjs",
    "jobs:refit": "node scripts/jobs/refit-weekly.mjs",
    "dev": "run-p dev:start dev:open",
    "dev:start": "next dev -p 3005",
    "dev:open": "wait-on http://localhost:3000 && node scripts/tools/open-browser.mjs",
    "build": "next build",
    "start": "next start",
    "test": "vitest --run",
    "test:e2e": "playwright test -c playwright.config.js",
    "test:e2e:ui": "playwright test --ui -c playwright.config.js",
    "score:rubric": "node scripts/score-rubric.mjs",
    "pm:pulse": "node scripts/tools/pm-pulse.mjs",
    "pm:close-superseded": "node scripts/tools/close-superseded-prs.mjs",
    "mcp:inspector": "npx @modelcontextprotocol/inspector codex mcp",
    "mcp:devtools": "npx chrome-devtools-mcp --target http://localhost:3000 --chrome-port 9222"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@mantine/charts": "^8.3.0",
    "@mantine/core": "^8.3.0",
    "@mantine/dropzone": "^8.3.0",
    "@mantine/hooks": "^8.3.0",
    "@mantine/modals": "^8.3.0",
    "@mantine/notifications": "^8.3.0",
    "@modelcontextprotocol/sdk": "^1.19.1",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-tabs": "^1.0.5",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@radix-ui/react-visually-hidden": "^1.0.3",
    "@reactflow/background": "^11.3.14",
    "@reactflow/controls": "^11.2.14",
    "@reactflow/node-resizer": "^2.2.14",
    "@supabase/supabase-js": "^2.45.4",
    "@tailwindcss/typography": "^0.5.19",
    "@tanstack/react-query": "^5.90.2",
    "@types/animejs": "^3.1.13",
    "animejs": "^4.2.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "d3": "^7.9.0",
    "d3-force": "^3.0.0",
    "dagre": "^0.8.5",
    "echarts": "^5.6.0",
    "framer-motion": "^12.23.22",
    "lucide-react": "^0.544.0",
    "next": "^15.0.0",
    "openai": "^6.2.0",
    "phosphor-react": "^1.4.1",
    "postcss-preset-mantine": "^1.18.0",
    "postcss-simple-vars": "^7.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "reactflow": "^11.11.4",
    "recharts": "^3.2.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@executeautomation/playwright-mcp-server": "^1.0.6",
    "@playwright/test": "^1.55.1",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/dagre": "^0.7.53",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "npm-run-all": "^4.1.5",
    "open": "^9.1.0",
    "postcss": "^8.5.6",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^4.1.13",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.20.6",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0",
    "wait-on": "^7.2.0"
  },
  "engines": {
    "node": ">=20.14.0 <21"
  }
}
