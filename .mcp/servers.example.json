{
  "mcpServers": {
    "codex": {
      "type": "stdio",
      "command": "codex",
      "args": ["mcp", "serve"]
    },
    "chrome-devtools": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "chrome-devtools-mcp",
        "--target",
        "http://localhost:3000",
        "--chrome-port",
        "9222"
      ]
    }
  }
}

